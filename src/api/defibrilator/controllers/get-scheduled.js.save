    'use strict';

    module.exports = {
      async getScheduled(ctx, next) {
        try {
          const data = await strapi.service('api::defibrilator.defibrilator').findAll();
  
          ctx.body = data;
        } catch (err) {
          ctx.badRequest('Page report controller error', { moreDetails: err })
        }
      }
    };
